<h1>New Form</h1>
<% Form::QUESTION_TYPES.each do |question_type| %>
  <ul>
    <li><%= question_type.humanize %></li>
  </ul>
<% end %>

<% if @question.errors.any? %>
    <h2>Errors:</h2>
    <ul>
      <% @question.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  <% end %>
<%= render 'partials/question_types/text' %>

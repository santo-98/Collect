<%= form_with(url: form_customers_path(form_id: @form.id), method: "post")do |form| %>
  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :phone_number %><br>
    <%= form.number_field :phone_number %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
